<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
       <bean name="company" class="Session15.Company">
           <constructor-arg name="companyName" value="Ivanov" type="java.lang.String"></constructor-arg>
           <constructor-arg name="aCeo"  ref="CEO1"></constructor-arg>
           <constructor-arg name="aCar" ref="Car1"></constructor-arg>

       </bean>
       <bean name="CEO1" class="Session15.CEO">
           <constructor-arg name="thisCar" ref="Car1"> </constructor-arg>
           <constructor-arg name="name" value="Vasylii Ivanovich Ivanov  "></constructor-arg>
       </bean>

       <bean name ="Car1" class="Session15.Car">
           <constructor-arg name="carName" value="Audi"></constructor-arg>


       </bean>



</beans>